{
  "name": "ventilab-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "validate:lessons": "node ./scripts/validate-lessons.mjs",
    "validate:lessons:fix": "node ./scripts/validate-lessons.mjs --fix",
    "validate:curriculum": "node ./scripts/validate-curriculum.mjs",
    "generate:modules": "node ./scripts/generate-missing-modules.mjs",
    "generate-lesson": "node scripts/generateLessonTemplate.js",
    "check:modules": "node ./scripts/audit-fix-module-ids.mjs",
    "audit:modules": "node ./scripts/audit-fix-module-ids.mjs --fix",
    "build:lessons-manifest": "node ./scripts/build-lessons-manifest.mjs",
    "prebuild": "npm run validate:curriculum && prisma generate",
    "prestart": "npm run build:lessons-manifest",
    "postinstall": "prisma generate && npm run build:lessons-manifest",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:seed": "ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' prisma/seed.ts",
    "db:seed": "ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google-cloud/storage": "^7.16.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.18.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "googleapis": "^149.0.0",
    "i18next": "^25.6.1",
    "katex": "^0.16.25",
    "next": "^16.1.1",
    "next-auth": "^4.24.13",
    "prisma": "^6.19.2",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^16.2.4",
    "react-markdown": "^10.1.0",
    "react-medium-image-zoom": "^5.4.0",
    "react-player": "^3.3.3",
    "react-router-dom": "^7.9.3",
    "react-svg-pan-zoom": "^3.13.1",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "swr": "^2.3.8",
    "zod": "^3.25.76",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.17.57",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "chalk": "^5.6.2",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "fast-glob": "^3.3.3",
    "prompts": "^2.4.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
